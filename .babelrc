{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
				},
				"modules": false
			}
		], ["@babel/preset-react"]
	],
	"plugins": [
		["@babel/plugin-transform-runtime"],
		["@babel/proposal-decorators", { "legacy": true }],
    	["@babel/proposal-class-properties", { "loose": true }]
	]
}